{
  "name": "causal/simple_api",
  "type": "typo3-cms-extension",
  "description": "Service to route HTTP/REST requests to your own TYPO3 controllers.",
  "keywords": [
    "TYPO3 CMS",
    "api"
  ],
  "homepage": "https://github.com/xperseguers/t3ext_simple_api",
  "support": {
    "issues": "https://github.com/xperseguers/t3ext_simple_api/issues"
  },
  "authors": [
    {
      "name": "Xavier Perseguers",
      "email": "xavier@causal.ch",
      "homepage": "https://www.causal.ch",
      "role": "Developer"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">= 8.2.0, <= 8.3.99",
    "typo3/cms-core": "^11 || ^12"
  },
  "autoload": {
    "psr-4": {
      "Causal\\SimpleApi\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Causal\\SimpleApi\\Tests": "Tests/"
    }
  },
  "replace": {
    "typo3-ter/simple-api": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-feature/TYPO3v10-v11": "1.2.x-dev"
    },
    "typo3/cms": {
      "extension-key": "simple_api"
    }
  }
}
